<?xml version="1.0" encoding="UTF-8"?>
<form validator="admin::com.wxform.form.validator.test"
	preprocessor="admin::com.wxform.form.preprocessor.test">
	<fieldset group="general" legend="general legend" label="general label">
		<text id="person" name="form[person]" size="20" default="test person"
			label="Person label" description="Person description" />
		<select id="access" name="form[access]" multiple="multiple"
			default="0" size="3" label="Access Level" description="DETAILACCESS">
			<option value="0">Public</option>
			<option value="1" label="Registered" />
			<option value="2" label="Special" />
		</select>
	</fieldset>
	<fieldset group="specific" legend="specific legend" label="specific label">
		<radiolist id="toto" name="form[toto]" default="1" size="2"
			label="toto label" description="toto description">
			<option value="1">Foo</option>
			<option value="2">Bar</option>
		</radiolist>
	</fieldset>
</form>